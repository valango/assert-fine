{
  "name": "assert-fine",
  "version": "1.1.2",
  "description": "A tiny utility for finding a cause of seemingly random failures in your code.",
  "main": "index.js",
  "homepage": "https://github.com/valango/assert-fine#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/valango/assert-fine.git"
  },
  "author": "Villem Alango <villem.alango@gmail.com>",
  "license": "MIT",
  "keywords": [
    "assert",
    "back-end",
    "backend",
    "breakpoint",
    "browser",
    "front-end",
    "frontend",
    "node.js",
    "lazy",
    "lightweight",
    "random failures",
    "state"
  ],
  "files": [
    "index.js",
    "browser.js",
    "fallback.js"
  ],
  "scripts": {
    "clean": "rimraf *.tmp *.l* reports",
    "lint": "eslint .",
    "purge": "npm run clean && rimraf node_modules",
    "testb": "jest --config test/node.config.js back-end.spec",
    "testf": "jest --config test/jest.config.js front-end.spec",
    "test": "npm run testb && npm run testf",
    "test:coverage": "codecov"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "codecov": "^3.8.1",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=10.15.1"
  }
}
